        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <PackageId>ch1seL.DistributedLock.MemoryLock</PackageId>
        <AssemblyName>ch1seL.DistributedLock.MemoryLock</AssemblyName>
